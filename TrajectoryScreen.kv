<TrajectoryScreen>:
    name: 'traj'
    FloatLayout:
        size_hint: None, None

    ImageButton:
        color: 1, 1, 1, 1
        x: root.width *0.5
        y: root.width * 0.5
        on_press: app.root.current = "settings"

    DPEAButton:
        id: test_button
        text:'back'
        size: 100, 100
        pos_hint: {"center_x": 0.5, "center_y":0.5}
        color: 0, 1, 0, 1
        on_press: root.switch_screen()