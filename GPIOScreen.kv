<GPIOScreen>:
    name: 'gpio'
    FloatLayout:
        size_hint: None, None

    DPEAButton:
        text: 'Home'
        font_size: 20
        size_hint: None, None
        size: 150, 50
        center_x: root.width*0.5
        center_y: root.height*0.6
        color: 0.67, 0.95, 0.54, 1
        on_press: root.endstop()

    DPEAButton:
        color: 0.7, 0.75, 0.78, 1
        size_hint: None, None
        size: 150, 100
        font_size: 20
        text: "Go Back"
        center_x: root.width *0.5
        y: root.height * 0.1
        on_press: root.switch_screen_settings()
