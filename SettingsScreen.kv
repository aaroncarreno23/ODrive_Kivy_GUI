<SettingsScreen>:
    name: 'settings'
    FloatLayout:
        size_hint: None, None

    DPEAButton:
        color: 0.7, 0.75, 0.78, 1
        size_hint: None, None
        size: 150, 100
        font_size: 20
        text: "Go Back"
        center_x: root.width *0.5
        y: root.height * 0.1
        on_press: root.switch_to_main()

    DPEAButton:
        color: 0.7, 0.75, 0.78, 1
        size_hint: None, None
        size: 150, 100
        font_size: 20
        text: "Rate"
        center_x: root.width *0.5
        y: root.height * 0.3
        on_press: root.switch_to_rate()

    DPEAButton:
        color: 0.7, 0.75, 0.78, 1
        size_hint: None, None
        size: 150, 100
        font_size: 20
        text: "GPIO"
        center_x: root.width *0.5
        y: root.height * 0.5
        on_press: root.switch_to_gpio()

    DPEAButton:
        color: 0.7, 0.75, 0.78, 1
        size_hint: None, None
        size: 150, 100
        font_size: 20
        text: "Trajectory"
        center_x: root.width *0.5
        y: root.height * 0.7
        on_press: root.switch_to_traj()



